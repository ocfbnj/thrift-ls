{
    "name": "thrift-ls",
    "version": "0.1.2",
    "publisher": "ocfbnj",
    "author": {
        "name": "ocfbnj"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ocfbnj/thrift-ls.git"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "license": "SEE LICENSE IN LICENSE",
    "displayName": "Thrift Language Server",
    "description": "Language server for Apache Thrift",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "thrift"
    ],
    "main": "./out/main.js",
    "contributes": {
        "languages": [
            {
                "id": "thrift",
                "aliases": [
                    "Thrift",
                    "thrift"
                ],
                "extensions": [
                    ".thrift"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "thrift",
                "scopeName": "source.thrift",
                "path": "./syntaxes/thrift.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "thrift_analyzer": "file:../../pkg",
        "vscode-languageclient": "^8.1.0",
        "vscode-languageserver": "^9.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@stylistic/eslint-plugin": "^4.2.0",
        "@types/node": "^20.11.24",
        "@types/vscode": "^1.85.0",
        "@vscode/vsce": "^2.24.0",
        "copy-webpack-plugin": "^13.0.0",
        "eslint": "^9.24.0",
        "ts-loader": "^9.5.2",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.29.1",
        "webpack": "^5.99.5",
        "webpack-cli": "^6.0.1"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm run wasm-pack && webpack",
        "wasm-pack": "wasm-pack build ../.. --target nodejs"
    },
    "icon": "icon.png"
}
